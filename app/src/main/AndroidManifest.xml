<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.haoge.sample.easyandroid">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light" >
        <activity android:name=".DemosActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.EasyToastActivity" />
        <activity android:name=".activities.EasyDimensionActivity" />
        <activity android:name=".activities.EasyFormaterActivity" />
        <activity android:name=".activities.EasyLogActivity" />
        <activity android:name=".activities.EasyReflectActivity" />
        <activity android:name=".activities.mvp.MVPDemoActivity" />
    </application>
</manifest>
